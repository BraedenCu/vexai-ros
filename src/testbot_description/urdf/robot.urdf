<?xml version="1.0"?>
<robot name="visual">

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0" radius="0"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0" radius="0"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>

  <link name="right_base_outside">
    <visual>
      <geometry>
        <box size="0.05 0.6 0.2"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.2286 0 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.6 0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>

  <link name="right_base_inside">
    <visual>
      <geometry>
        <box size="0.05 0.6 0.2"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.1524"/>
      <material name="white"/>
    </visual>
     <collision>
      <geometry>
        <box size="0.05 0.6 0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>

  <link name="left_base_outside">
    <visual>
      <geometry>
        <box size="0.05 0.6 0.2"/>
      </geometry>
      <origin rpy="0 0 0" xyz="-0.2286 0 0"/>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.05 0.6 0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>


  <link name="camera_link">
    <visual>
      <geometry>
        <box size="0.0254 0.1524 0.1143"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.1143"/>
      <material name="blue"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.0254 0.1524 0.1143"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>



  <joint name="base_link_to_right_base_outside" type="fixed">
    <parent link="base_link"/>
    <child link="right_base_outside"/>
    <origin xyz="0.2286 0 0"/>
  </joint>

  <joint name="base_link_to_left_base_outside" type="fixed">
    <parent link="base_link"/>
    <child link="left_base_outside"/>
    <origin xyz="-0.2286 0 0"/>
  </joint>

  <joint name="base_link_to_camera" type="fixed">
    <parent link="base_link"/>
    <child link="camera_link"/>
    <origin xyz="0 0 0.1143"/>
  </joint>

</robot>
